<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Database_Classes.Loginlog" table="LOGINLOG" schema="CHENYAN">
        <composite-id mapped="true" class="Database_Classes.LoginlogPK">
            <key-property name="userid" column="USERID"/>
            <key-property name="logintime" column="LOGINTIME"/>
        </composite-id>
    </class>
</hibernate-mapping>